    "@types/node": "^22.0.0",
    "@types/node-notifier": "^8.0.5",
    "@types/uuid": "^11.0.0",
    "eslint": "^9.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.54.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
