  "files": [
    "dist/",
    "skills/",
    "loops/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@slack/bolt": "^4.6.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.0",
    "gray-matter": "^4.0.3",
    "node-notifier": "^10.0.1",
    "pptxgenjs": "^4.0.1",
